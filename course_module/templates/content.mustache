{{!
    @template blocks/course_module.
}}
{{#data}}
        <div class="p-1">
            <a class="text-decoration-none" href="{{{config.wwwroot}}}/mod/{{name}}/view.php?id={{id}}">
                <span>
                    {{id}}
                </span>
                -
                <span>
                    {{module_name}}
                </span>
                -
                <span>
                    {{date_created}}
                </span>
                {{#completed}}
                    -
                    <span>
                        {{#str}}completed, block_course_module{{/str}}
                    </span>
                {{/completed}}
            </a>
        </div>
{{/data}}
